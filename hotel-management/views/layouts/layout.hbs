    <script type="text/javascript">
      function calculateTotal(){
        var val1 = document.getElementById('price').value;
        var v1 = val1.substring(16,45);
        var val2 = document.getElementById('quant').value;
        var total = v1 * val2;
        document.getElementById("total").innerHTML =  + total;
      }
      window.onload = calculateTotal;
    </script>
